<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<!-- 
#######################################
#                                      
#     공용 설정 정보      
#                                     
#######################################
 -->
<properties>
	<!--데이터 베이스 프로퍼티 설정 경로  -->
	<entry key="databasePropertiesHost">NETHOSTING</entry>
	<entry key="databasePropertiesPath">classpath:config/database/developer</entry> 
	<entry key="databasePropertiesMode">1</entry> <!-- (0=file : 파일을 통해 읽어옴, 1=ddns 호스트 서버에서 읽어옴, 2=data : 인증서버를 통해 읽어옴) -->
	<entry key="databaseAccessLogWrite">0</entry> <!-- 0:안함/1:기록 [databasePropertiesMode 2인경만 해당 cnct_dttm 와 ip를 기록하도록 한다] -->
	
	<!-- # 이미지ftp서버의 접속 정보 시작 -->
	<entry key="ftp.image.serverType">1</entry> <!-- ftp타입 (0=ftp, 1=sftp) -->
	<entry key="ftp.image.host">182.162.143.62</entry>
	<entry key="ftp.image.port">8422</entry>
	<entry key="ftp.image.user">images</entry>
	<entry key="ftp.image.password">http://images.c0.kr</entry>
	<!-- 이미지 ftp서버의 업로드 경로 -->
	<entry key="ftp.image.rootDir">/system</entry>
	<!-- 이미지 ftp서버 url -->
	<entry key="ftp.image.url">http://images.sky.com</entry>
	
</properties>